nk v-bind:to=\"{name: 'add-review',params: {id: product.id}}\">\n        Add Review\n      </router-link>\n    \n    </div>\n    <div class=\"well-display\">\n      <average-summary />\n      <star-summary rating=\"1\" />\n      <star-summary rating=\"2\" />\n      <star-summary rating=\"3\" />\n      <star-summary rating=\"4\" />\n      <star-summary rating=\"5\" />\n    </div>\n    <review-list />\n  </div>\n</template>\n\n<script>\nimport AverageSummary from \"@/components/AverageSummary.vue\";\nimport StarSummary from \"@/components/StarSummary.vue\";\nimport ReviewList from \"@/components/ReviewList.vue\";\n\nexport default {\n  //lifecycle hook that will be executed every time this componenet is created\n  created() {\n    const activeProductId = this.$route.params.id;\n    this.$store.commit(\"SET_ACTIVE_PRODUCT\", activeProductId);\n  },\n  computed: {\n    product() {\n      return this.$store.state.products.find(\n        (p) => p.id == this.$store.state.activeProduct\n      );\n    },\n  },\n  components: {\n    AverageSummary,\n    StarSummary,\n    ReviewList,\n  },\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/views"}]}